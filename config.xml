<?xml version='1.2' encoding='utf-8'?>
<widget id="batleaf1.autoloop.app.com" version="1.2.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>UMOJaPAAP!</name>
    <description>
        Umoja PAAP
    </description>
    <author email="eliud_ngugi@bat.com" href="http://cordova.io">
       UmojaPaap
    </author>
    <content src="index.html" />
    <icon src="icon.png" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-vibration" spec="~2.1.3" />
    <engine name="android" spec="~6.1.0" />
</widget>
